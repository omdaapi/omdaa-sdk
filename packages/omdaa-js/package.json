{
  "name": "omdaa-api-client",
  "version": "1.1.1",
  "description": "Official JavaScript/TypeScript client for Omdaa WhatsApp Business API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "omdaa",
    "whatsapp",
    "api",
    "client",
    "sdk"
  ],
  "author": "Omdaa",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omdaapi/omdaa-sdk.git"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}