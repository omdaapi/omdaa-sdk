{
  "name": "omdaa/omdaa-php",
  "description": "Official PHP client for Omdaa WhatsApp Business API",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [{"name": "Omdaa", "homepage": "https://omdaa.com"}],
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Omdaa\\Api\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Omdaa\\Api\\Tests\\": "tests/"
    }
  },
"scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "keywords": ["omdaa", "whatsapp", "api", "client", "sdk"]
}
