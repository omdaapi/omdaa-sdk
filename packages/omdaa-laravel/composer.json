{
  "name": "omdaa/omdaa-laravel",
  "description": "Laravel integration for Omdaa WhatsApp Business API",
  "type": "library",
  "license": "MIT",
  "authors": [{ "name": "Omdaa", "homepage": "https://omdaa.com" }],
  "repositories": [
    { "type": "path", "url": "../omdaa-php", "options": { "symlink": true } }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "illuminate/config": "^9.0|^10.0|^11.0",
    "omdaa/omdaa-php": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Omdaa\\Laravel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Omdaa\\Laravel\\OmdaaServiceProvider"
      ],
      "aliases": {
        "Omdaa": "Omdaa\\Laravel\\OmdaaFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
